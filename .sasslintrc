#########################
## Sample Sass Lint File
#########################
# Linter Options
options:
  merge-default-rules: true
rules:
  leading-zero:
    - 1
    - include: true
  quotes:
    - 1
    - style: 'double'
  property-sort-order:
    - 1
    - order: 'recess'
    - ignore-custom-properties: true
